page {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
.main {
    position: relative;
    width: 100%;
}
.main navigator image {
    width: 100%;
    height: 480rpx;
}
.main input {
    position: absolute;
    top: 40rpx;
    left: 80rpx;
    height: 40rpx;
    width: 80%;
    border: 1px solid white;
    border-radius: 40rpx;
}
swiper {
    height: 238px;
}
.row {
    display: flex;
    justify-content: space-around;
    align-self: center;
    flex-wrap: wrap;
}
.row-col{
    width: 360rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.row-col image {
    display: flex;
    max-width: 100%;
}
.scroll {
    display: flex;
    align-self: center;
    justify-content: space-between;
    align-items: center;
}
.scroll .left {
    flex: 2;
    height: 100px;
    padding: 10rpx;
}
.scroll .right {
    flex: 1;
    height: 100px;
    padding: 10rpx;
}
.blank {
    width: 100%;
    height: 10px;
    background-color: white;
}
.circle_container {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-around;
    padding: 0 10rpx;
}
.circle_min .image {
    height: 120rpx;
    width: 120rpx;
    border: 1px solid white;
    border-radius: 40px;
}
.text {
    font-size: 14px;
}