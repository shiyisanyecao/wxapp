.container {
    position: relative;
    width: 100%;
}
.container input {
    position: absolute;
    background-color: #e9e9e9;
    top: 20rpx;
    left: 20rpx;
    width: 80%;
    height: 30rpx;
}
.container p {
    position: absolute;
    right: 20rpx;
    top: 19rpx;
}
.row {
    position: absolute;
    top: 100rpx;
    display: flex;
    justify-content: space-around;
    align-self: center;
    flex-wrap: wrap;
}
.row-col{
    width: 360rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.row-col image {
    display: flex;
    max-width: 100%;
}