.box text {
    font-size: 15px;
}
.box .box_hd {
  position: relative;
  left: 20rpx;
}
.circle_detail {
    display:flex;
    justify-content: space-around;
    flex-wrap: wrap;
}
.nav {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 10rpx 20rpx;
}
.nav image {
    height: 120rpx;
    width: 120rpx;
    border: 1px solid white;
    border-radius: 40px;
}
.weui-cell.head {
    padding: 8px 14px;
  }
  .weui-cell.head image {
    width: 134rpx;
    height: 134rpx;
    margin-right: 16px;
    margin-left: 0;
  }
  .name {
    margin-top: 2rpx;
  }
  .stu_id {
    font-size: 30rpx;
    margin-top: 8rpx;
    color: #999;
  }
  
  .weui-cell__hd {
    font-size: 0;
  }
  .weui-cell__hd image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 18px;
    margin-left: 5px;
    vertical-align: middle;
  }
.weui-cell p {
    font-size: 10px;
}